{
  "name": "aichatbase",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "up": "docker compose up",
    "down": "docker compose down && docker image prune -f && docker builder prune -f",
    "db:migrate:apply": "docker exec -it aichatbase_app npx prisma migrate deploy",
    "db:migrate:dev": "docker exec -it aichatbase_app npx prisma migrate dev",
    "db:migrate:reset": "docker exec -it aichatbase_app npx prisma migrate reset --skip-seed",
    "db:migrate:status": "docker exec aichatbase_app npx prisma migrate status",
    "db:migrate:resolve": "prisma migrate resolve --applied",
    "db:migrate:prod": "docker compose exec aichatbase_app npx prisma migrate deploy",
    "db:generate": "docker compose exec aichatbase_app npx prisma generate",
    "db:studio": "docker compose exec aichatbase_app npx prisma studio",
    "db:seed": "docker compose exec aichatbase_app npx prisma db seed",
    "db:format": "docker exec aichatbase_app npx prisma format"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.7.0",
    "@aws-sdk/client-s3": "^3.670.0",
    "@aws-sdk/s3-presigned-post": "^3.670.0",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.20.0",
    "@radix-ui/react-avatar": "^1.1.1",
    "@radix-ui/react-checkbox": "^1.1.2",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-scroll-area": "^1.2.0",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.1",
    "@radix-ui/react-tabs": "^1.1.1",
    "@shadcn/ui": "^0.0.4",
    "@stripe/stripe-js": "^4.8.0",
    "@tanstack/react-table": "^8.20.5",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "geist": "^1.3.1",
    "lucide-react": "^0.452.0",
    "next": "14.2.15",
    "next-auth": "^4.24.8",
    "next-themes": "^0.3.0",
    "react": "^18",
    "react-dom": "^18",
    "recharts": "^2.13.3",
    "stripe": "^17.2.0",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20.16.11",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.15",
    "postcss": "^8",
    "prisma": "^5.20.0",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  }
}
